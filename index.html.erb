<h1 class="header"> KUJU'S TASK MANAGER </h1>
<%@task_lists.each do |task_list| %>
  <div class="index_row clearfix">
    <h2 class="task_list_title"> <%= link_to task_list.title, task_list %> </h2>
    <p class="task_list_sub_title"><%= task_list.description %> </p>
  </div>
  <% end %>

  <div class="links new_task_btn">
    <%= link_to "New Task List", new_task_list_path %>
  </div>